<%- include ../partials/header.html %>
<h3><a class="btn btn-secondary" href="/vehicle_document/add"><i class="fa fa-plus"></i> Create New</a> Vehicle Document List</h3>
<table class="table table-striped">
    <thead>
        <tr>
            <th>SL NO</th>
            <th>Vehicle Id</th>
            <th>Image</th>
            <th>Registration Type</th>
            <th>Registration No</th>
            <th>Registration Date</th>         
            <th>Expiry Date</th>
           
            <th></th>
        </tr>
    </thead>
    <tbody>
        <% var i=1;
        list.forEach(function(val) { %>
        <tr>
            <td> <%= i %></td>
            
            <td><% vehicle.forEach(function(veh) {
                if(veh._id==val.vehicle_id){ %>
                <%= veh.model_name %>
                <% } }); %></td>

            <td><%= val.image %></td> 
            <!-- <td> <img <% if(val.image && val.image!=''){%>src="<%= baseurl %>/public/uploads/<%= val.user_image %>" <%} else{%> src="<%= baseurl %>myimages/no_photo.jpg"<%}%> height="50px" alt="">
            </td> -->
            <td><%= val.registration_type %></td>
            <td><%= val.registration_no %></td>
            <td><%= val.registration_date %></td>
            <td><%= val.expiry_date %></td>
            <td>
                <a href="/vehicle_document/editvehicledoc/<%= val._id %>"><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
                <a href="/vehicle_document/delete/<%= val._id %>" onclick="return confirm('Are you sure to delete this record ?');"><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
            </td>
        </tr>
        <% i++; }) %>
    </tbody>
</table>

<%- include ../partials/footer.html %>
